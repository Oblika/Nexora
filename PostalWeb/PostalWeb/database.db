CREATE DATABASE paketadb;

USE paketadb

CREATE TABLE paketat (
    numriPaketes VARCHAR(50) NOT NULL PRIMARY KEY,
    emriDerguesit VARCHAR(100) NOT NULL,
    kohaArdhjes VARCHAR(50) NOT NULL, 
    vendodhjaPakos VARCHAR(100),
    emriMarresit VARCHAR(100),
    numriTelefonit VARCHAR(15),
    adresa VARCHAR(255)
);


